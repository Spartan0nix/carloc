{% extends "admin/base.html.twig" %}

{% block title 'Ajouter une couleur' %}

{% block body %}
<main id='admin_color_add' class='admin_crud_create'>
    <div class="header">
        <h2>Ajouter une couleur</h2>
    </div>
    <div class="form">
    {{ form_start(form) }}
        <div class="input-text">
            <label>Nom :</label>
            {{ form_widget(form.color)}}
        </div>
        <div class="submit">
            {{ form_widget(form.submit) }}
        </div>
    {{ form_end(form) }}
    </div>
</main>

{% endblock %}